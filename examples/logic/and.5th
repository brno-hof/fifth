# n1 n2 -> (n1 && n2)
and:
  if # n1 = true (<=> n1 > 0)
    swap
    if # n2 = true (<=> n2 > 0)
      pop
    else # n2 = false (<=> n2 = 0)
      swap
      pop
    then
  else # n1 = false (<=> n1 = 0)
    swap
    pop
  then
return

